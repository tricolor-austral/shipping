generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Shipment {
  id          String   @id @default(cuid())
  createdAt   DateTime @default(now())
  dueDate     DateTime
  orderID     String
  destiny     String
  status Status @default(DISPATCHED)
}


enum Status {
  DISPATCHED
  PROGRESS
  DELIVERED

}